{
  "name": "Hairsalons",
  "version": "0.1.1",
  "description": "Application",
  "main": "index.js",
  "author": "Daniel Wallin",
  "scripts": {
    "mongo:start": "mongod --config /usr/local/etc/mongod.conf --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "build:clean": "rimraf dist/client/static",
    "build-stage": "webpack --config ./webpack/config.stage.js --progress --profile --colors",
    "start-stage": "yarn run build-stage && NODE_ENV=stage node index.js",
    "start": "yarn run build-stage && NODE_ENV=stage node index.js",
    "dev": "yarn run build:clean && NODE_ENV=development node index.js",
    "test": "jest --coverage"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "src"
    ],
    "testRegex": ".spec.js$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "config": "^1.21.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^1.1.11",
    "hapi": "^13.4.1",
    "hapi-auth-cookie": "^6.1.1",
    "hapi-webpack-plugin": "^1.3.0",
    "inert": "^4.0.0",
    "json-loader": "^0.5.7",
    "mongodb": "^3.1.10",
    "mongoose": "^4.5.3",
    "node-sass": "^4.9.0",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-test-renderer": "^16.7.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.0",
    "redux-mock-store": "^1.5.3",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-jest": "^23.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0"
  }
}
